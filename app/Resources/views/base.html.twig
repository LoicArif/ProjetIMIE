<!DOCTYPE html>
<html>
<head>
    <title>{% block title %} {% endblock %} - Geek NJOB</title>

    {% block head %}
        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <link rel="icon" type="image/png" href="{{ asset('favicon.png') }}">
        {# Jquery #}
        <script rel="script" src="{{ asset('assets/jquery/jquery-3.2.1.js') }}"></script>
        {#<script rel="script" src="{{ asset('assets/jquery/jquery-3.2.1.min.js') }}"></script>#}
    {% endblock %}

    {% block stylesheets %}
        {# Boostrap #}
        <link rel="stylesheet" href="{{ asset('assets/bootstrap/css/bootstrap.css') }}">
        <link rel="stylesheet" href="{{ asset('assets/font-awesome/css/font-awesome.css') }}"/>
        <link rel="stylesheet" href="{{ asset('assets/css/matrix-style.css') }}"/>
        <link rel="stylesheet" href="{{ asset('assets/css/matrix-media.css') }}"/>
        <link rel="stylesheet" href="{{ asset('assets/css/main.css') }}"/>
        <link href="https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css" rel="stylesheet" integrity="sha384-wvfXpqpZZVQGK6TAh5PVlGOfQNHSoD2xbE+QkPxCAFlNEevoEH3Sl0sibVcOQVnN" crossorigin="anonymous">
    {% endblock %}
</head>
<body>

{% block header %}

{% endblock %}

{% block content %}

{% endblock %}

{% block footer %}

{% endblock %}

{% block javascripts %}
    <script src="{{ asset('assets/js/admin/matrix.js') }}"></script>
    {# Bootstrap #}
    <script src="{{ asset('assets/bootstrap/js/bootstrap.js') }}"></script>
{% endblock %}

</body>
</html>

